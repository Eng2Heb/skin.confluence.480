<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">10</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>{{ScaleX('335')}}</left>
		<top>{{ScaleY('230')}}</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<include name="DialogBackgroundCommons">
			<param name="DialogBackgroundWidth" value="{{ScaleX('610')}}" />
			<param name="DialogBackgroundHeight" value="{{ScaleY('240')}}" />
			<param name="DialogHeaderWidth" value="{{ScaleX('530')}}" />
			<param name="DialogHeaderLabel" value="$LOCALIZE[13406]" />
			<param name="DialogHeaderId" value="1" />
			<param name="CloseButtonLeft" value="{{ScaleX('520')}}" />
			<param name="CloseButtonNav" value="10" />
		</include>
		<control type="textbox" id="9">
			<description>text</description>
			<left>{{ScaleX('30')}}</left>
			<top>{{ScaleY('62')}}</top>
			<width>{{ScaleX('550')}}</width>
			<height>{{ScaleY('98')}}</height>
