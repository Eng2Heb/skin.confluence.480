<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">10</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>{{ScaleX('335')}}</left>
		<top>{{ScaleY('230')}}</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<include name="DialogBackgroundCommons">
			<param name="DialogBackgroundWidth" value="{{ScaleX('610')}}" />
			<param name="DialogBackgroundHeight" value="{{ScaleY('240')}}" />
			<param name="DialogHeaderWidth" value="{{ScaleX('530')}}" />
			<param name="DialogHeaderLabel" value="$INFO[Control.GetLabel(1)] $INFO[System.Progressbar,- ,%]" />
			<param name="DialogHeaderId" value="0" />
			<param name="CloseButtonLeft" value="{{ScaleX('520')}}" />
			<param name="CloseButtonNav" value="10" />
		</include>
		<control type="label" id="1">
			<description>fake heading label</description>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('0')}}</top>
			<width>{{ScaleX('0')}}</width>
			<height>{{ScaleY('0')}}</height>
			<font>-</font>
			<visible>false</visible>
		</control>
		<control type="textbox" id="9">
			<description>text</description>
			<left>{{ScaleX('30')}}</left>
			<top>{{ScaleY('62')}}</top>
			<width>{{ScaleX('550')}}</width>
			<height>{{ScaleY('78')}}</height>
			<align>left</align>
			<label>-</label>
			<font>font13</font>
			<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
		</control>
		<control type="progress" id="20">
			<description>Progressbar</description>
			<left>{{ScaleX('30')}}</left>
			<top>{{ScaleY('148')}}</top>
			<width>{{ScaleX('550')}}</width>
			<height>{{ScaleY('16')}}</height>
			<info>System.Progressbar</info>
		</control>
		<control type="button" id="10">
			<description>Cancel button</description>
			<left>{{ScaleX('205')}}</left>
			<top>{{ScaleY('175')}}</top>
			<width>{{ScaleX('200')}}</width>
			<height>{{ScaleY('40')}}</height>
			<label>222</label>
			<font>font12_title</font>
			<textcolor>white</textcolor>
			<focusedcolor>white</focusedcolor>
			<align>center</align>
			<onleft>10</onleft>
			<onright>10</onright>
			<onup>10</onup>
			<ondown>10</ondown>
		</control>
	</controls>
</window>
