<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>5</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>{{ScaleX('290')}}</left>
		<top>{{ScaleY('75')}}</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<include name="DialogBackgroundCommons">
			<param name="DialogBackgroundWidth" value="{{ScaleX('700')}}" />
			<param name="DialogBackgroundHeight" value="{{ScaleY('570')}}" />
			<param name="DialogHeaderWidth" value="{{ScaleX('620')}}" />
			<param name="DialogHeaderLabel" value="$LOCALIZE[20043]" />
			<param name="DialogHeaderId" value="2" />
			<param name="CloseButtonLeft" value="{{ScaleX('610')}}" />
			<param name="CloseButtonNav" value="5" />
		</include>
		<control type="grouplist" id="5">
			<description>control area</description>
			<left>{{ScaleX('30')}}</left>
			<top>{{ScaleY('70')}}</top>
			<width>{{ScaleX('640')}}</width>
			<height>{{ScaleY('410')}}</height>
			<itemgap>5</itemgap>
			<onleft>28</onleft>
			<onright>29</onright>
			<onup>28</onup>
			<ondown>28</ondown>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('0')}}</top>
			<height>{{ScaleY('40')}}</height>
			<font>font13</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus border="5">button-focus2.png</texturefocus>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('0')}}</top>
			<height>{{ScaleY('40')}}</height>
			<font>font13</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus border="5">button-focus2.png</texturefocus>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('0')}}</top>
			<height>{{ScaleY('40')}}</height>
			<font>font13</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturefocus border="5">button-focus2.png</texturefocus>
			<texturenofocus border="5">button-nofocus.png</texturenofocus>
			<aligny>center</aligny>
			<reverse>yes</reverse>
		</control>
		<control type="image" id="11">
			<description>separator image</description>
			<height>{{ScaleY('2')}}</height>
			<texture>separator2.png</texture>
		</control>
		<control type="label" id="14">
			<description>Default Label</description>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('0')}}</top>
			<height>{{ScaleY('30')}}</height>
			<label>-</label>
			<align>center</align>
			<aligny>bottom</aligny>
			<font>font13_title</font>
			<textcolor>grey</textcolor>
			<shadowcolor>black</shadowcolor>
		</control>
		<control type="button" id="28">
			<description>Ok Button</description>
			<left>{{ScaleX('145')}}</left>
			<top>{{ScaleY('505')}}</top>
			<width>{{ScaleX('200')}}</width>
			<height>{{ScaleY('40')}}</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font12_title</font>
			<label>186</label>
			<onleft>29</onleft>
			<onright>29</onright>
			<onup>5</onup>
			<ondown>5</ondown>
		</control>
		<control type="button" id="29">
			<description>Cancel Button</description>
			<left>{{ScaleX('355')}}</left>
			<top>{{ScaleY('505')}}</top>
			<width>{{ScaleX('200')}}</width>
			<height>{{ScaleY('40')}}</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font12_title</font>
			<label>222</label>
			<onleft>28</onleft>
			<onright>28</onright>
			<onup>5</onup>
			<ondown>5</ondown>
		</control>
	</controls>
</window>
